<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cm.xingyu.chat"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.cm.xingyu.chat.MainActivity" />
        <activity android:name="com.cm.xingyu.chat.SignInActivity" />
        <activity android:name="com.cm.xingyu.chat.LoginInActivity" />
        <activity android:name="com.cm.xingyu.chat.FragmentTabHostActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.cm.xingyu.chat.ChatInterfaceActivity" />
    </application>

</manifest>